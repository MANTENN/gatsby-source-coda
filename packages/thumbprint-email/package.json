{
    "name": "@thumbtack/thumbprint-email",
    "version": "0.1.0-beta.7",
    "author": "Tom Genoni <tom@thumbtack.com>",
    "main": "dist/thumbprint-email.css",
    "style": "dist/thumbprint-email.css",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/thumbtack/thumbprint.git"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    },
    "license": "Apache-2.0",
    "engines": {
        "node": ">=8"
    },
    "scripts": {
        "start": "yarn clear && yarn generate-css",
        "clear": "rm -rf dist",
        "generate-css": "mkdir -p dist && node-sass -q src/thumbprint-email.scss dist/thumbprint-email.css --importer=./node-sass-importer.js",
        "prepublishOnly": "../../scripts/should-build-package.js -- yarn start"
    },
    "devDependencies": {
        "@thumbtack/thumbprint-tokens": "^12.1.0",
        "node-sass": "^4.13.1",
        "node-sass-tilde-importer": "2.0.0-alpha.1"
    },
    "bugs": {
        "url": "https://github.com/thumbtack/thumbprint/issues"
    },
    "homepage": "https://github.com/thumbtack/thumbprint/blob/master/packages/thumbprint-email/"
}
